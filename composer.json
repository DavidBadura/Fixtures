{
    "name": "davidbadura/fixtures",
    "description": "A library to load yaml, json or php fixtures. Easy extendable, resolve object dependency and validate objects.",
    "keywords": ["fixtures", "yaml", "json", "events", "database", "doctrine", "mongodb", "orm", "faker", "puli"],
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name" : "David Badura",
        "email" : "d.badura@gmx.de"
    }],
    "require": {
        "php": ">=5.3.0",
        "symfony/event-dispatcher" : "^2.7|^3.0",
        "symfony/finder" : "^2.7|^3.0"
    },
    "require-dev": {
        "symfony/yaml" : "^2.7|^3.0",
        "symfony/validator": "^2.7|^3.0",
        "symfony/http-kernel": "^2.7|^3.0",
        "fzaninotto/faker": "~1.1",
        "doctrine/common": "~2.0",
        "jamesmoss/toml": "*",
        "symfony/expression-language": "^2.7|^3.0"
    },
    "suggest": {
        "symfony/yaml" : "^2.7|^3.0",
        "symfony/validator": "^2.7|^3.0",
        "symfony/http-kernel": "^2.7|^3.0",
        "fzaninotto/faker": "~1.1",
        "doctrine/orm": "~2.0",
        "doctrine/mongodb-odm": "@beta",
        "jamesmoss/toml": "*",
        "symfony/expression-language": "^2.7|^3.0"
    },
    "autoload": {
        "psr-4": {
            "DavidBadura\\Fixtures\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DavidBadura\\Fixtures\\": "tests"
        }
    }
}
