    "type": "library",
    "license": "MIT",
    "authors": [{
        "name" : "David Badura",
        "email" : "david.badura@patchlevel.de"
    }],
    "require": {
        "php": "^7.4|^8.0",
        "symfony/event-dispatcher" : "^4.4|^5.3|^6.0",
        "symfony/finder" : "^4.4|^5.3|^6.0"
    },
    "require-dev": {
        "patchlevel/coding-standard": "^1.1.1",
        "symfony/yaml" : "^4.4|^5.3|^6.0",
        "symfony/validator": "^4.4|^5.3|^6.0",
        "symfony/http-kernel": "^4.4|^5.3|^6.0",
        "fzaninotto/faker": "^1.1.0",
        "doctrine/common": "^2.0|^3.0",
        "doctrine/collections": "^1.6",
        "jamesmoss/toml": "^1.1",
        "symfony/expression-language": "^4.4|^5.3|^6.0",
        "phpunit/phpunit": "^9.5.4",
        "symfony/config": "^4.4|^5.3|^6.0",
        "symfony/security-core": "^4.4|^5.3|^6.0",
        "symfony/dependency-injection": "^4.4|^5.3|^6.0",
        "symfony/console": "^4.4|^5.3|^6.0",
        "phpspec/prophecy-phpunit": "^2.0.1",
        "phpstan/phpstan": "^1.2.0",
        "vimeo/psalm": "^4.13.1"
    },
    "suggest": {
        "symfony/yaml" : "^4.4|^5.3|^6.0",
        "symfony/validator": "^4.4|^5.3|^6.0",
        "symfony/http-kernel": "^4.4|^5.3|^6.0",
        "fzaninotto/faker": "~1.1",
        "doctrine/orm": "^2.0|3.0",
        "doctrine/mongodb-odm": "^1.2",
        "jamesmoss/toml": "^1.1",
        "symfony/expression-language": "^4.4|^5.3|^6.0"
    },
    "autoload": {
        "psr-4": {
            "DavidBadura\\Fixtures\\": "src"
        }
